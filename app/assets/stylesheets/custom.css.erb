html, body {min-height:100%; background:#f2f2f2; margin: 0; padding: 0;}

.fl{float:left;}
.fr{float:right;}
.cl{clear: both}

body p, body ol, body ul, body td{
    font-size:15px;
    line-height: 24px;
    font-family:'latoregular', 'Open Sans', Verdana, sans-serif;
    padding-bottom: 10px;
    padding-top: 10px}

body a{color: #2FC3F0;}

h3{font-size: 30px; line-height:36px; margin-top: 0; font-family:'latolight', 'Open Sans', Verdana, sans-serif;}
h4{font-family:'latobolds', 'Open Sans', Verdana, sans-serif;}

.navbar-toggle{margin-top: 30px; margin-bottom: 30px;}
.navbar-inverse .navbar-toggle .icon-bar{background-color: #48494A!important;}
.navbar-inverse .navbar-toggle{border-color: #48494A!important;}
.navbar-inverse .navbar-toggle:hover{background-color: #25B7F0; border-color: #25B7F0!important;}

#main{background:#F4F4F4; margin:0 auto; text-align: center; border-bottom:1px solid #fff;}
#main img{max-width:95%; margin: 0 auto; display: block; padding: 45px 0;}
#main_bottom{background:#5A5B5D repeat; padding: 40px 20px; margin: 0 auto; text-align: center;}
#main_bottom p{color: #fff; font-size: 14px; line-height:24px; max-width: 840px; margin: 0 auto; padding:15px 0;}
#main_bottom h2{color: #fff; font-size: 30px; text-transform: uppercase; line-height: 36px;}
#main_bottom a{color: #20b5ee; transition: color 0.5s ease;}
#main_bottom a:hover{color: #7edcff; transition: color 0.5s ease; text-decoration: none; background: none;}

.navbar{height: auto; min-height: 90px; margin: 0; background: #fff; border-bottom:2px solid #20b5ee;}
.navbar-brand{height: auto;}
.navbar-nav {float: right; font-family:'latolight', 'Open Sans', Verdana, sans-serif; text-transform: uppercase;}
.navbar-inverse .navbar-nav > li > a{color: #5b5c5d; transition: color 0.5s ease;}
.navbar-inverse .navbar-nav > li > a:hover{color: #20b5ee; transition: color 0.5s ease;}
.nav-pills > li > a:hover{background-color: #4A4B4C;}

.supermain{padding-top: 30px;}

@media (min-width: 768px) {
	.navbar-nav > li > a {padding-top: 30px;padding-bottom: 30px;}
}

.business-header {
    height: 500px;
    padding-top: 50px;
    background: url(<%= asset_path('hero.jpg') %>) center center no-repeat scroll;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    background-size: cover;
    -o-background-size: cover;}

.tagline {
	font-family:'latolight','Open Sans', Verdana, sans-serif;
    font-weight: 100;
    font-size:50px;
    color: #fff;
    line-height: 60px;
    background-color: rgba(0, 0, 0, 0.6);
    display: inline;}

p.homecta{margin-top: 50px;}
p.homecta .btn{
	background: #20b5ee;
	transition: background 0.5s ease;
	color:#fff;
	font-family: 'latobold', 'Open Sans', Verdana, sans-serif;
	font-size: 24px;
	text-transform: uppercase;
	border-radius: 0!important;}
p.homecta .btn:hover{background: #2FC3F0; transition: background 0.5s ease; color:#fff;}

.img-center {margin: 0 auto;}

.container.homec{margin-top:40px;}
.container.homec h2{
    font-family:'latolight','Open Sans', Verdana, sans-serif;
    font-size:60px;
    line-height: 65px;
    color:#333;
    text-align: center;
    margin-top: 20px;}
h2.tabh{padding-bottom: 60px;}
.container.homec .row{margin-top: 35px; margin-bottom: 35px}

.detab .nav-pills > li.active > a, .motab .nav-pills > li.active > a{
    background-color: #25B6F0;
}

.detab, .motab {padding-bottom:50px; clear: both; display: block; background-color: #f2f2f2; margin: 60px 0 50px;}
.detab img, .motab img{padding: 0 30px 20px;}
.detab p, .motab p{font-size:15px; line-height: 24px; padding-bottom: 10px; padding-top: 10px;}
.detab a, .motab a{font-size:16px; line-height: 24px;}


.nav-pills > li > a{border-radius: 0;}


footer {margin: 50px 0 0; background-color: #f2f2f2; color: #5b5c5d;}
footer h4 {margin-top:40px; font-weight:bold; margin-bottom: 20px;}
footer p{margin:0; padding:0; font-size:13px;}
footer a{color:#2FC3F0!important; transition: color 0.5s ease;}
footer a:hover{color:#00A0DB!important; transition: color 0.5s ease; text-decoration: none; background-color: transparent;}
footer img{padding:20px 10px 10px; float: left;}

footer .bluefoot{background-color: #20b5ee; color: #fff;}
footer .bluefoot ul{padding: 10px 30px; clear: both;}
footer .bluefoot li{
    list-style: none;
    padding: 8px;
    border-bottom: 1px solid;
    border-color:rgba(255, 255, 255, 0.5);
    font-size: 13px;}
footer .bluefoot li a{color: #fff; color: rgba(255, 255, 255, 1.0)!important; transition: color 0.5s ease;}
footer .bluefoot li a:hover{
    text-decoration: none;
    background-color: transparent;
    color: #fff;
    color: rgba(255, 255, 255, 0.7)!important;
    transition: color 0.5s ease;}
footer .bluefoot li:last-child{border: none;}
footer .bluefoot h4 {margin: 40px 30px 10px; font-weight:bold; clear: both;}

.supermain.login{background-color: #f2f2f2; height: 100%; margin: 0; padding-bottom: 30px;}
.supermain.login > .container{
    max-width: 450px;
    width: auto;
    margin: 0 auto;
}
.supermain.login > .container img{padding:5px 0 20px;}

.login-logo{
    background-color: #fff;
    border:1px solid #e5e5e5;
    margin-top: 50px;
    padding: 10px 20px 15px;}

.supermain.login h2{
    font-family:'latolight', 'Open Sans', Verdana, sans-serif;
    text-align: center;
    font-size:48px;
    line-height: 52px;
}

.supermain.login input{
    height: 55px;
    font-size:30px;
    font-family:'latolight', 'Open Sans', Verdana, sans-serif;
    color: #7e7f81;
    padding: 10px;
    width: 100%;
    margin:0;
    border:none;
    background-color: #f2f2f2;
    border:1px solid #e5e5e5;
    line-height: normal;
}

.supermain.login input[type="checkbox"]{
    height: auto;
    padding: 0;
    float: left;
    margin: 2px 3px 2px 0;
    width: auto;
}

.supermain.login .col-md-8 {padding-left: 0; margin-top: 15px;}

.supermain.login .col-md-8 label{
    font-family:'latobold', 'Open Sans', Verdana, sans-serif;
    text-transform: uppercase;
    color: #494a4b;
}

div.field{margin-bottom: 0!important;}

.actions input[type="submit"]{
    font-family:'latobold', 'Open Sans', Verdana, sans-serif;
    background: #16a4ed;
    transition: background 0.5s ease;
    color: #fff;
    border: none;
    font-size:30px;
    margin-top: 30px;
    margin-bottom: 15px;
    padding: 10px 5px;}

.actions input[type="submit"]:hover{
    background: #2fc2f8;
    transition: background 0.5s ease; }

.login-logo a{text-align: center; color:#16a4ed!important; transition: color 0.5s ease; display: block; }
.login-logo a:hover{color: #2DC0F7!important; transition: color 0.5s ease; text-decoration: none; background: none;}

